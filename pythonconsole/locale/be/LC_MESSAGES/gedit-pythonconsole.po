#~ msgid "Candidate Encodings"
#~ msgstr "Прапануемыя кадаванні"

#~ msgid ""
#~ "List of candidate encodings shown in the Character Encoding menu in the "
#~ "open/save file chooser. “CURRENT” represents the current locale encoding. "
#~ "Only recognized encodings are used. The default value is the empty list, "
#~ "in which case gedit will choose good defaults depending on the country "
